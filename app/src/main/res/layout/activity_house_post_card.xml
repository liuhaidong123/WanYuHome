<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_house_post_card"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    tools:context="com.home.wanyu.activity.HousePostCardActivity">

    <RelativeLayout
        android:id="@+id/add_address_title"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/white">

        <ImageView
            android:id="@+id/house_post_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:src="@mipmap/property_back" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center"
            android:text="@string/house_post"
            android:textStyle="bold"
            android:textColor="@color/title_color"
            android:textSize="17sp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/house_prompt_num"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@id/add_address_title"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text=""
        android:textColor="@color/eac6"
        android:textSize="14sp" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/house_prompt_num"
        android:layout_marginTop="5dp"
        android:scrollbars="none">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/house_rent_type_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_rent_type_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_type"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/house_rent_img_all"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/house_rent_type_tv"
                    android:src="@mipmap/select_house_rent" />

                <TextView
                    android:id="@+id/house_rent_all"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/house_rent_img_all"
                    android:text="@string/house_rent_type_all"
                    android:textColor="@color/titlecolor3"
                    android:textSize="14sp" />


                <ImageView
                    android:id="@+id/house_rent_img_other"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/house_rent_all"
                    android:src="@mipmap/select_house_rent_no" />

                <TextView
                    android:id="@+id/house_rent_other"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/house_rent_img_other"
                    android:text="@string/house_rent_type_other"
                    android:textColor="@color/titlecolor3"
                    android:textSize="14sp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_area_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_rent_type_rl"
                android:layout_marginTop="5dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_area_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_area"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:id="@+id/select_city_rl"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_area_tv"
                    android:background="@drawable/house_rent_all_bg">

                    <TextView
                        android:id="@+id/house_city_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="5dp"
                        android:text=""
                        android:textColor="@color/titlecolor3" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="5dp"
                        android:src="@mipmap/house_more" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_small_area_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_area_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_small_area_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_name"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_small_area_tv"
                    android:background="@drawable/house_rent_all_bg">

                    <EditText
                        android:id="@+id/area_name"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_centerVertical="true"
                        android:background="@null"
                        android:hint="@string/house_rent_hint_small_area"
                        android:inputType="text"
                        android:paddingLeft="5dp"
                        android:textColor="@color/titlecolor3"
                        android:textColorHint="@color/repair_line"
                        android:textSize="14sp" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_type_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_small_area_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_type_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_huType"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_type_tv"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/house_shi_edit"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="3"
                            android:background="@null"
                            android:inputType="number"
                            android:paddingLeft="5dp"
                            android:textColor="@color/titlecolor3"
                            android:textColorHint="@color/repair_line"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/house_shi"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/house_shi"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="25dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/house_ting_edit"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="3"
                            android:background="@null"
                            android:inputType="number"
                            android:paddingLeft="5dp"
                            android:textColor="@color/titlecolor3"
                            android:textColorHint="@color/repair_line"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/house_ting"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:text="@string/house_ting"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="25dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/house_wei_edit"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="3"
                            android:background="@null"
                            android:inputType="number"
                            android:paddingLeft="5dp"
                            android:textColor="@color/titlecolor3"
                            android:textColorHint="@color/repair_line"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/house_wei"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:text="@string/house_wei"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_wo_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_small_area_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/house_wo_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_wo"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:id="@+id/house_wo_relative"
                    android:layout_width="126dp"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/house_wo_tv"
                    android:background="@drawable/house_rent_all_bg">

                    <TextView
                        android:id="@+id/house_wo_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="5dp"
                        android:text=""
                        android:textColor="@color/titlecolor3" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="5dp"
                        android:src="@mipmap/house_more" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_m_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_type_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_m_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_m"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="126dp"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_m_tv"
                    android:background="@drawable/house_rent_all_bg"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/house_m_edit"
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_weight="3"
                        android:background="@null"
                        android:inputType="number"
                        android:paddingLeft="5dp"
                        android:textColor="@color/titlecolor3"
                        android:textColorHint="@color/repair_line"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/house_m"
                        android:textColor="@color/titlecolor3"
                        android:textSize="14sp" />

                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_louceng_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_m_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_louceng_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_lou"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_louceng_tv"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="第"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />


                        <EditText
                            android:id="@+id/house_ceng_edit"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="3"
                            android:background="@null"
                            android:inputType="number"
                            android:textColor="@color/titlecolor3"
                            android:textColorHint="@color/repair_line"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="层"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="25dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="共"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />


                        <EditText
                            android:id="@+id/house_all_ceng_edit"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="3"
                            android:background="@null"
                            android:inputType="number"
                            android:paddingLeft="5dp"
                            android:textColor="@color/titlecolor3"
                            android:textColorHint="@color/repair_line"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="层"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />
                    </LinearLayout>

                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_xiang_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_louceng_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_xiang_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_direction"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <LinearLayout
                    android:id="@+id/direction_ll"
                    android:layout_width="77.5dp"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_xiang_tv"
                    android:background="@drawable/house_rent_all_bg"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/direction_tv_msg"
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text=""
                        android:textColor="@color/titlecolor3"
                        android:textSize="14sp" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:src="@mipmap/house_more" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_configure_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/house_xiang_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_configure_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/house_rent_configure"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />


                <com.home.wanyu.myview.MyGridView
                    android:id="@+id/house_configure_gridview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignTop="@id/house_configure_tv"
                    android:paddingBottom="15dp"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/house_configure_tv"
                    android:numColumns="4"
                    android:verticalSpacing="10dp">
                </com.home.wanyu.myview.MyGridView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_money_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_configure_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_money_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_money"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_money_tv"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">


                        <EditText
                            android:id="@+id/house_money_edit"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_weight="3"
                            android:background="@null"
                            android:inputType="number"
                            android:paddingLeft="5dp"
                            android:textColor="@color/titlecolor3"
                            android:textColorHint="@color/repair_line"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="45dp"
                            android:layout_marginRight="5dp"
                            android:gravity="center"
                            android:text="元/月"
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/pay_monet_type_ll"
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginLeft="25dp"
                        android:layout_weight="1"
                        android:background="@drawable/house_rent_all_bg"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/pay_money_tv"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text=""
                            android:textColor="@color/titlecolor3"
                            android:textSize="14sp" />


                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="5dp"
                            android:src="@mipmap/house_more" />
                    </LinearLayout>

                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_img_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/house_money_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_img_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/house_rent_add_img"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />


                <com.home.wanyu.myview.MyGridView
                    android:id="@+id/house_img_gridview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@id/house_img_tv"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="15dp"
                    android:numColumns="4"
                    android:verticalSpacing="10dp">

                </com.home.wanyu.myview.MyGridView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_phone_name_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_img_rl"
                android:layout_marginTop="5dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_phone_name_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_phone_person"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <EditText
                    android:id="@+id/house_phone_name_edit"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_phone_name_tv"
                    android:background="@null"
                    android:hint="输入姓名"
                    android:paddingLeft="5dp"
                    android:textColor="@color/titlecolor3"
                    android:textColorHint="@color/repair_line"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_phone_num_rl"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@id/house_phone_name_rl"
                android:layout_marginTop="5dp"
                android:background="@color/white">

                <TextView
                    android:id="@+id/house_phone_num_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="@string/house_rent_phone_num"
                    android:textColor="@color/color9"
                    android:textSize="14sp" />

                <EditText
                    android:id="@+id/house_phone_num_edit"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/house_phone_num_tv"
                    android:background="@null"
                    android:hint="输入手机号"
                    android:inputType="number"
                    android:maxLength="11"
                    android:paddingLeft="5dp"
                    android:textColor="@color/titlecolor3"
                    android:textColorHint="@color/repair_line"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/house_phone_num_rl"
                android:layout_marginTop="1dp"
                android:background="@color/white">

                <Button
                    android:id="@+id/house_post_submit"
                    android:layout_width="325dp"
                    android:layout_height="45dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="40dp"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/order_select_btn"
                    android:text="@string/house_rent_submit"
                    android:textColor="@color/eac6" />
            </RelativeLayout>

        </RelativeLayout>
    </ScrollView>


</RelativeLayout>
