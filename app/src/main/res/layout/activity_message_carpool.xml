<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_message_carpool"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/homefragmentFgbac"
    tools:context="com.home.wanyu.activity.MessageCarpoolActivity">
    <TextView
        android:id="@+id/fg3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/homefragmentFgColor"/>
<ListView
    android:paddingBottom="10dp"
    android:scrollbars="none"
    android:divider="@null"
    android:cacheColorHint="#00000000"
    android:layout_below="@+id/fg3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/message_carpool_listview">
</ListView>
</RelativeLayout>
