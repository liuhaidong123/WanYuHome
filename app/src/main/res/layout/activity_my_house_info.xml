<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_house_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:background="@color/homefragmentFgColor"
    tools:context="com.home.wanyu.activity.MyHouseInfoActivity">

    <RelativeLayout
        android:background="@drawable/banck"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
    <!--城市-->
        <RelativeLayout
            android:id="@+id/city_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/add_address_title"
            android:layout_marginTop="5dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">
            <TextView
                android:id="@+id/city_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_city"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/city_tv_msg_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/city_tv"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/circle_small_back" />
            <TextView
                android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/homefragmentFgColor"/>
        </RelativeLayout>
    <!--小区-->
        <RelativeLayout
            android:id="@+id/area_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/city_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/area_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_area"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/area_tv_msg_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/area_tv"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@mipmap/circle_small_back" />
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>
        </RelativeLayout>
    <!--业主姓名-->
        <RelativeLayout
            android:id="@+id/name_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/area_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">
            <TextView
                android:id="@+id/name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="我的姓名"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <EditText
                android:id="@+id/name_tv_msg_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/name_tv"
                android:background="@null"
                android:maxLength="10"
                android:inputType="text"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp" />
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>
        </RelativeLayout>
<!--手机号-->
        <RelativeLayout
            android:id="@+id/phone_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/name_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/phone_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_phone"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <EditText
                android:id="@+id/phone_tv_msg_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/phone_tv"
                android:background="@null"
                android:inputType="number"
                android:maxLength="11"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp" />
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>
        </RelativeLayout>
        <!--身份-->
        <RelativeLayout
            android:id="@+id/phone_rl_pri_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/phone_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/phone_tv_pri"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="我        是"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />
            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/phone_tv_pri"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/phone_tv_msg_pri_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:maxLength="11"
                    android:layout_toLeftOf="@+id/sele"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/homeFragChangeColor"
                    android:textSize="13sp" />
                <TextView
                    android:id="@+id/sele"
                android:textSize="14sp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="选择"
                android:layout_alignParentRight="true"
                android:textColor="@color/bg_rect"/>
            </RelativeLayout>
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>
        </RelativeLayout>
<!--小区楼号-->
        <RelativeLayout
            android:id="@+id/lou_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/phone_rl_pri_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/lou_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_lou"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tta"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="号楼"
                android:textSize="13sp"/>
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>

            <EditText
                android:layout_toLeftOf="@+id/tta"
                android:layout_toRightOf="@+id/lou_tv"
                android:maxLength="3"
                android:layout_marginRight="10dp"
                android:id="@+id/lou_tv_msg_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:background="@null"
                android:maxLines="4"
                android:gravity="center_vertical"
                android:inputType="number"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp"
                android:layout_alignBaseline="@+id/lou_tv"
                android:layout_alignBottom="@+id/lou_tv"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
<!--楼层-->
        <RelativeLayout
            android:id="@+id/ceng_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/lou_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/ceng_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_ceng"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="层"
                android:textSize="13sp"
                android:id="@+id/textView4" />
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>

            <EditText
                android:layout_toRightOf="@+id/ceng_tv"
                android:id="@+id/ceng_tv_msg_1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toLeftOf="@+id/textView4"
                android:background="@null"
                android:maxLines="1"
                android:maxLength="3"
                android:gravity="center_vertical"
                android:inputType="number"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp"
              />
        </RelativeLayout>
<!--单元-->
        <RelativeLayout
            android:id="@+id/unit_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/ceng_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/unit_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_unit"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <EditText
                android:id="@+id/unit_tv_msg_1"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/unit_tv"
                android:background="@null"
                android:maxLines="13"
                android:maxLength="10"
                android:inputType="number"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp" />
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="单元"
                android:textSize="13sp"/>
            <TextView
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/homefragmentFgColor"/>
        </RelativeLayout>
<!--房号-->
        <RelativeLayout
            android:id="@+id/hourse_rl_1"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/unit_rl_1"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/hourse_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/add_hourse"
                android:textColor="@color/homeFragTopTextColor"
                android:textSize="13sp" />

            <EditText
                android:maxLength="12"
                android:id="@+id/hourse_tv_msg_1"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/hourse_tv"
                android:background="@null"
                android:maxLines="1"
                android:layout_toLeftOf="@+id/ta"
                android:inputType="number"
                android:textColor="@color/homeFragChangeColor"
                android:textSize="13sp" />
            <TextView
                android:id="@+id/ta"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="号房"
                android:textSize="13sp"/>
        </RelativeLayout>
    </RelativeLayout>

