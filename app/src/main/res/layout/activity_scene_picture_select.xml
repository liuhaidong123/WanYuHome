<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_scene_picture_select"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/homefragmentFgColor"
    tools:context="com.home.wanyu.activity.ScenePictureSelectActivity">
    <include
        android:id="@+id/scenepicture_layout"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        layout="@layout/activity_titleinclude_btn">
    </include>
    <GridView
        android:layout_marginTop="1dp"
        android:layout_below="@+id/scenepicture_layout"
        android:verticalSpacing="1dp"
        android:horizontalSpacing="1dp"
        android:listSelector="#00ffeeaa"
        android:numColumns="3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scenepicture_gridView">

    </GridView>
</RelativeLayout>
