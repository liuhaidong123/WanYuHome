<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color">

    <RelativeLayout
        android:id="@+id/content_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <RelativeLayout
            android:id="@+id/state_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/express_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/express_state"
                android:textColor="@color/title_color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/express_state_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/express_state"
                android:text=""
                android:textColor="@color/bg_rect"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/express_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:text=""
                android:textColor="@color/name_color"
                android:textSize="12sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/zitigui_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/state_rl"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/express_zi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="@string/express_zi"
                android:textColor="@color/name_color"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/express_zi_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/express_zi"
                android:text=""
                android:textColor="@color/title_color"
                android:textSize="14sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tihuoma_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/zitigui_rl"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/express_ma"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/express_ma"
                android:textColor="@color/name_color"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/express_ma_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/express_ma"
                android:text=""
                android:textColor="@color/bg_rect"
                android:textSize="14sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tihuoma_from"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tihuoma_rl"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/express_from"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/express_from"
                android:textColor="@color/name_color"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/express_from_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/express_from"
                android:text=""
                android:textColor="@color/titlecolor3"
                android:textSize="14sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tihuoma_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tihuoma_from"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/express_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/express_num"
                android:textColor="@color/name_color"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/express_num_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/express_num"
                android:text=""
                android:textColor="@color/titlecolor3"
                android:textSize="14sp" />

        </RelativeLayout>


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/end_rl"
        android:layout_width="match_parent"
        android:layout_height="38dp"
        android:layout_below="@id/content_rl"
        android:layout_marginTop="1dp"
        android:background="@color/white">

        <TextView
            android:id="@+id/end_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text=""
            android:textColor="@color/bg_rect"
            android:textSize="15sp" />

    </RelativeLayout>

</RelativeLayout>